<!--start page-->
<section class="sectionhome">

    <!--start container-->
    <div class="container clearfix">


        <div class="grid_12"></div>
        <div class="grid_12"></div>
        <!--40px height-->


        <div class="grid_12">
            <div class="titlesection red">
                <h2 class="titlewithborder"><span>Packages</span></h2>
            </div>
        </div>

        <!--start blogmasonry-->
        <div class="stylemasonry">

            @foreach (var package in Model)
            {
                <!--start post masonry-->
                <div id="dynamicClass" class="grid_4 singlemasonry singlepostmasonry red">
                    
                    <img alt="" class="imgsinglepostmasonry" src="~/@package.ProfilePicSource.Substring(1)">

                    <div class="titledaysinglepostmasonry">

                        <a href="/Package/Details/@package.Id">
                            <p class="titlesinglepostmasonry">@package.Name</p>
                        </a>
                    </div>

                    @if (package.Description == null)
                    {
                        <p class="descriptionsinglepostmasonry">No Details Available</p>
                    }
                    else if (package.Description.Length > 160)
                    {
                        <p class="descriptionsinglepostmasonry">@package.Description.Substring(0, 160)<span>...</span></p>
                    }
                    else
                    {
                        <p class="descriptionsinglepostmasonry">@package.Description</p>
                    }
                    

                    <a class="areadmoresinglepostmasonry" href="/Package/Details/@package.Id">
                        <p class="readmoresinglepostmasonry">READ MORE..</p>
                    </a>

                </div>
                <!--end post masonry-->
            }
            
            

        </div>
        <!--end blogmasonry-->

    </div>
    <!--end container-->

</section>
<!--end internal page-->

